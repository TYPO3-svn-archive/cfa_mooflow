                  <type>select</type>
                  <items type="array">
                    <numIndex index="0" type="array">
                      <numIndex index="0">LLL:EXT:cfa_mooflow/locallang_db.xml:cfa_mooflow.pi_flexform.mode.0</numIndex>
                      <numIndex index="1">MANUAL</numIndex>
